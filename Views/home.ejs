    <!--====== Post Area Start ======-->
    <section class="post-area no-sidebar">
        <div class="container-fluid">
            <div class="post-area-inner">
                <!-- Entry Post -->
                <div class="entry-posts two-column masonary-posts row">
                    <!--  -->
                    <%blogs.forEach(b=>{%>
                    <div class="col-xl-3 col-lg-4 col-sm-6">
                        <div class="entry-post">
                            <div class="entry-thumbnail">
                                <img src="/uploads/<%= b.image %>" alt="Image">
                            </div>
                            <div class="entry-content">
                                <h4 class="title">
                                    <a href="/blogDetails/<%= b.id %>">
                                        <%= b.title %>
                                    </a>
                                </h4>
                                <ul class="post-meta">
                                    <li class="date">
                                        <a href="#">18-04-21</a>
                                    </li>
                                    <li class="categories">
                                        <%= b.author %>
                                    </li>
                                </ul>
                                <p>
                                    <%=b.description%>
                                </p>
                                <a href="/blogDetails/<%= b.id %>" class="read-more">
                                    Read More <i class="fas fa-long-arrow-right"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <%})%>
                    <!--  -->
                    <div class="col-12">
                        <div class="text-center">
                            <a href="#" class="load-more-btn">Load More</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
